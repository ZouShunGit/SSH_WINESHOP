<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
		<!-- 1系统常量 -->
	<!-- 设置视图主题为默认 -->
	<constant name="struts.ui.theme" value="simple" />
	<!-- 指后后缀名为.action -->
	<constant name="struts.action.extension" value="action" />
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="false" />
	<!-- <constant name="struts.multipart.saveDir" value="/tmp"/> -->
	<!-- struts2 action实例化对象的转移权spring -->
	<package name="studio" namespace="/studio" extends="struts-default">
	
	</package>
	
	<include file="strutsActionMapper/userMapper.xml"></include>
	<include file="strutsActionMapper/adminMapper.xml"></include>
	<include file="strutsActionMapper/deptMapper.xml"></include>
	<include file="strutsActionMapper/emplMapper.xml"></include>
	<include file="strutsActionMapper/empMapper.xml"></include>
	<include file="strutsActionMapper/categoryMapper.xml"></include>
	<include file="strutsActionMapper/scategoryMapper.xml"></include>
	<include file="strutsActionMapper/clothingMapper.xml"></include>
	<include file="strutsActionMapper/setMapper.xml"></include>
	<include file="strutsActionMapper/carMapper.xml"></include>
	<include file="strutsActionMapper/commentsMapper.xml"></include>
	<include file="strutsActionMapper/orderMapper.xml"></include>
	<include file="strutsActionMapper/samplecategpryMapper.xml"></include>
	<include file="strutsActionMapper/sampleMapper.xml"></include>
	<include file="strutsActionMapper/photoMapper.xml"></include>
	<include file="strutsActionMapper/indexMapper.xml"></include>
	
</struts>